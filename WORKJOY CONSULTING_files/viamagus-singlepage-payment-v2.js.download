    submitSinglePagePayment: function() {
        var that = this;
        var paymentAmount = "6999";
        var successUrl = "https://viamagus.net/register/paymentSuccess.html?txnId=";
        var cancelUrl = location.href;
        var failureUrl = "https://viamagus.net/register/paymentFailure.html?txnId=";
        $('#vmWebsitePaymentButton').attr("disabled", "disabled");
        var paymentMode = "razorpay";
        var txnCurrency = 'INR';
        var productInfoJson = '';
        var country, state, city, countryCode = "";
        var reqOptions = {
            url: that._paymentUrl + '/REST/payment/saveSinglePagePayment',
            data: {
                format: 'json',
                type: 'POST',
                paymentAmount: paymentAmount,
                productName: 'Viamagus Single Page Fee',
                customerName: $('#paymentCustName').val(),
                customerEmail: $('#paymentCustEmail').val(),
                customerPhoneNo: $('#paymentCustPhoneNo').val(),
                addressLineOne: '',
                addressLineTwo: '',
                state: state,
                city: city,
                country: country,
                countryCode: countryCode,
                pincode: '',
                successUrl: successUrl,
                cancelUrl: cancelUrl,
                failureUrl: failureUrl,
                entityId: that._businessId,
                entityType: 'website',
                callBackProcess: '',
                originUrl: location.href,
                paymentMode: paymentMode,
                productInfoJson: productInfoJson,
                txnCurrency: txnCurrency,
                isTestMode: 'Y'
            },
            callback: function(data, uniqueId) {
                if (paymentMode == 'razorpay' && data.result.paymentGateway == 'razorpay') {
                    that.loadRazorPay(data, uniqueId, data.result.paymentGateway);
                }
                $('#vmPaymentModal').modal('hide');
            },
            errorCallBack: function() {
                $('#vmWebsitePaymentButton').removeAttr("disabled");
                alert("Transaction Failed.");
            },
            async: false
        };
        new viamagusTransactionManager(reqOptions);
    },
    loadRazorPay: function(data, uniqueId, paymentGateway) {
        var that = this;
        var razorPayUrl = "https://checkout.razorpay.com/v1/checkout.js";
        var script = document.createElement("script")
        script.type = "text/javascript";
        if (script.readyState) {
            script.onreadystatechange = function() {
                if (script.readyState == "loaded" || script.readyState == "complete") {
                    script.onreadystatechange = null;
                    that.invokeRazorPay(data, uniqueId, paymentGateway);
                }
            }
            ;
        } else {
            script.onload = function() {
                that.invokeRazorPay(data, uniqueId);
            }
            ;
        }
        script.src = razorPayUrl;
        document.body.appendChild(script);
    },
    invokeRazorPay: function(data, uniqueId, paymentGateway) {
        var that = this;
        var razorPayOptions = {
            "key": data.result.apiKey,
            "amount": parseFloat(6999 * 100),
            "name": data.result.merchantName,
            "description": uniqueId,
            "image": "",
            "handler": function(response) {
                new viamagusLoader('body');
                that.submitToRazorPayHandler(data, uniqueId, paymentGateway, response.razorpay_payment_id);
            },
            "prefill": {
                "name": $('#paymentCustName').val(),
                "email": $('#paymentCustEmail').val(),
                "contact": $('#paymentCustPhoneNo').val().replace(/ /g, "").replace("+", "")
            },
            "notes": {
                "txnId": uniqueId
            },
            "theme": {
                "color": "#F37254"
            }
        };
        new Razorpay(razorPayOptions).open();
    },
    submitToRazorPayHandler: function(data, uniqueId, paymentMode, razorPayId) {
        var that = this;
        this.submitToUrl(data.result.paymentVerficationUrl, {
            txnid: uniqueId,
            amount: 6999,
            razorpayId: razorPayId,
            productinfo: 'Viamagus Single Page Fee',
            firstname: $('#customerName').val(),
            email: $('#customerEmail').val(),
            paymentMode: paymentMode
        });
    },
    submitToUrl: function(path, params, method) {
        method = method || "post";
        var form = document.createElement("form");
        form._submit_function_ = form.submit;
        form.setAttribute("method", method);
        form.setAttribute("action", path);
        for (var key in params) {
            var hiddenField = document.createElement("input");
            hiddenField.setAttribute("type", "hidden");
            hiddenField.setAttribute("name", key);
            hiddenField.setAttribute("value", params[key]);
            form.appendChild(hiddenField);
        }
        document.body.appendChild(form);
        form._submit_function_();
    },
